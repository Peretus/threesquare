<%= form_for @place do |f| %>
	<%=  f.label :location, "Location Name" %>
	<%=   f.text_field :location, :placeholder => "Grand Canyon" %>

	<%=  f.label :url, "Image URL" %>
	<%=   f.text_field :url, :placeholder => "http://img_url.com/" %>

	<%=  f.label :description %>
	<%=   f.text_field :description, :placeholder => "This place is amazing!" %>

	<%= f.submit "Go!" %>
	
<% end %>